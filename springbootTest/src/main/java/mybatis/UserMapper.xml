<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wsx.springbootTest.dao.UserDao">
 
  <select id="findUserInfo" parameterType="String" resultType="com.wsx.springbootTest.domain.User" >
    select name, age from user1 where id= #{id}
  </select>
 
  <select id="getUserList" resultType="User">
  select id ,name ,age from user1
  </select>
  
    <select id="getUser" resultType="User" parameterType="String">
  select id ,name ,age from user1 where id = #{id}
  </select>
  
  <update id="addUser" parameterType="User">
  insert into user1 (name,age) values (#{user.name},#{user.age})
  </update>
  
  <update id="updateUser" parameterType="User">
  update user1 set name = #{user.name},age=#{user.age} where id = #{user.id}
  </update>
  
  <delete id="deleteUser" parameterType="String">
  delete from user1 where id = #{id}
  </delete>
  
  <select id="getPhoneAndNickname" parameterType="int" resultType="Customer">
  select phone as phone ,nickname as nickname from temp limit #{start},#{pageSize}
  </select>
 
</mapper>